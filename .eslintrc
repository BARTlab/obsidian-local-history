{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": [
        "src/**/*.js"
      ]
    }
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": [
          "warn",
          "error",
          "info"
        ]
      }
    ],
    "@typescript-eslint/comma-dangle": "off",
    "comma-dangle": "off",
    "comma-spacing": "off",
    "spaced-comment": "off",
    "camelcase": "off",
    "no-extra-boolean-cast": "off",
    "new-cap": ["error", { "capIsNew": false }],
    "linebreak-style": ["error", "unix"],
    "func-call-spacing": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_|Inject|ServiceToken"
      }
    ],
    "operator-linebreak": "off",
    "no-invalid-this": "off",
    "semi": "off",
    "@typescript-eslint/dot-notation": "off",
    "@typescript-eslint/no-inferrable-types": "off",
    "brace-style": [
      "off",
      "off"
    ],
    "padding-line-between-statements": [
      "error",
      {
        "blankLine": "always",
        "prev": "block-like",
        "next": "*"
      },
      {
        "blankLine": "always",
        "prev": "*",
        "next": "block-like"
      },
      {
        "blankLine": "always",
        "prev": "*",
        "next": "return"
      },
      {
        "blankLine": "always",
        "prev": "multiline-const",
        "next": "*"
      },
      {
        "blankLine": "always",
        "prev": "multiline-let",
        "next": "*"
      },
      {
        "blankLine": "always",
        "prev": "multiline-var",
        "next": "*"
      }
    ],
    "import/order": "off",
    "no-underscore-dangle": "off",
    "object-shorthand": "off",
    "indent": 0,
    "require-jsdoc": 0,
    "array-bracket-newline": [
      "warn",
      "consistent"
    ],
    "array-bracket-spacing": [
      "warn",
      "never"
    ],
    "array-element-newline": [
      "warn",
      "consistent"
    ],
    "max-params": [
      "warn",
      4
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "max-len": [
      "warn",
      {
        "ignorePattern": "^import |^export | implements | className",
        "code": 120,
        "tabWidth": 2
      }
    ],
    "arrow-spacing": "warn",
    "block-spacing": "warn",
    "function-call-argument-newline": [
      "warn",
      "consistent"
    ],
    "space-before-function-paren": [
      "warn",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "quote-props": [
      "warn",
      "consistent"
    ],
    "@typescript-eslint/explicit-member-accessibility": [
      "warn",
      {
        "accessibility": "explicit"
      }
    ],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "arrow-parens": [
      "warn",
      "always"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "@typescript-eslint/consistent-type-imports": [
      "warn",
      {
        "prefer": "type-imports"
      }
    ]
  }
}
