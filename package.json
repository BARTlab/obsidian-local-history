{
  "name": "obsidian-line-change-tracker",
  "version": "1.0.0",
  "description": "Tracks and highlights changed lines in the editor with markers, show version diff, make local history for files changes.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "editor",
    "diff",
    "changes"
  ],
  "author": "B@RT",
  "license": "MIT",
  "devDependencies": {
    "@shellscape/esbuild-sass-plugin": "^2.4.4-patch.2",
    "@types/node": "^16.11.6",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "builtin-modules": "3.3.0",
    "diff": "^8.0.2",
    "diff2html": "^3.4.51",
    "esbuild": "0.17.3",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.2",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "eventemitter3": "^5.0.1",
    "lodash-es": "^4.17.21",
    "reflect-metadata": "^0.2.2"
  },
  "dependencies": {
  }
}
